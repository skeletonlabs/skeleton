{
	"name": "@skeletonlabs/skeleton-svelte",
	"version": "4.7.2",
	"description": "The Svelte package for Skeleton.",
	"author": "endigo9740 <chris@skeletonlabs.dev>",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/skeletonlabs/skeleton.git",
		"directory": "packages/skeleton-svelte"
	},
	"scripts": {
		"build": "svelte-package --input src --output dist",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"prepack": "pnpm build && clean-package",
		"postpack": "clean-package restore"
	},
	"exports": {
		".": {
			"types": "./src/index.ts",
			"import": "./src/index.ts",
			"svelte": "./src/index.ts",
			"style": "./src/index.css"
		}
	},
	"files": [
		"dist"
	],
	"sideEffects": false,
	"dependencies": {
		"@internationalized/date": "catalog:",
		"@skeletonlabs/skeleton-common": "workspace:*",
		"@zag-js/accordion": "catalog:",
		"@zag-js/avatar": "catalog:",
		"@zag-js/collapsible": "catalog:",
		"@zag-js/collection": "catalog:",
		"@zag-js/combobox": "catalog:",
		"@zag-js/date-picker": "catalog:",
		"@zag-js/dialog": "catalog:",
		"@zag-js/file-upload": "catalog:",
		"@zag-js/floating-panel": "catalog:",
		"@zag-js/listbox": "catalog:",
		"@zag-js/menu": "catalog:",
		"@zag-js/pagination": "catalog:",
		"@zag-js/popover": "catalog:",
		"@zag-js/progress": "catalog:",
		"@zag-js/radio-group": "catalog:",
		"@zag-js/rating-group": "catalog:",
		"@zag-js/slider": "catalog:",
		"@zag-js/steps": "catalog:",
		"@zag-js/svelte": "catalog:",
		"@zag-js/switch": "catalog:",
		"@zag-js/tabs": "catalog:",
		"@zag-js/tags-input": "catalog:",
		"@zag-js/toast": "catalog:",
		"@zag-js/toggle-group": "catalog:",
		"@zag-js/tooltip": "catalog:",
		"@zag-js/tree-view": "catalog:"
	},
	"peerDependencies": {
		"svelte": "^5.29.0"
	},
	"devDependencies": {
		"@skeletonlabs/skeleton": "workspace:*",
		"@sveltejs/kit": "catalog:",
		"@sveltejs/package": "catalog:",
		"@sveltejs/vite-plugin-svelte": "catalog:",
		"@vitest/browser-playwright": "catalog:",
		"clean-package": "catalog:",
		"svelte": "catalog:",
		"svelte-check": "catalog:",
		"typescript": "catalog:",
		"vite": "catalog:",
		"vitest-browser-svelte": "catalog:"
	},
	"license": "MIT",
	"type": "module",
	"clean-package": {
		"remove": [
			"scripts",
			"devDependencies",
			"clean-package"
		],
		"replace": {
			"exports": {
				".": {
					"types": "./dist/index.d.ts",
					"import": "./dist/index.js",
					"svelte": "./dist/index.js",
					"style": "./dist/index.css"
				}
			}
		}
	}
}
