		"test:watch": "vitest watch"
	},
	"devDependencies": {
		"@types/estree": "^1.0.8",
		"@types/node": "^24.0.14",
		"@types/semver": "^7.7.0",
		"type-fest": "^4.41.0",
		"typescript": "catalog:",
		"unbuild": "^3.5.0",
		"vitest": "^3.2.4"
	},
	"license": "MIT",
	"dependencies": {
		"@clack/prompts": "^0.11.0",
		"colorette": "^2.0.20",
		"commander": "^14.0.0",
		"detect-indent": "^7.0.1",
		"latest-version": "^9.0.0",
		"magic-string": "^0.30.17",
		"nanoid": "^5.1.5",
		"node-html-parser": "^7.0.1",
		"package-manager-detector": "^1.3.0",
		"postcss": "^8.5.6",
		"semver": "^7.7.2",
		"svelte": "catalog:",
		"tinyglobby": "^0.2.14",
		"ts-morph": "^26.0.0",
		"zimmerframe": "^1.1.2"
	}
