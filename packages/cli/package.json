{
	"name": "skeleton",
	"description": "The CLI for Skeleton related tooling.",
	"version": "1.0.0",
	"repository": {
		"type": "git",
		"url": "https://github.com/skeletonlabs/skeleton",
		"directory": "packages/cli"
	},
	"sideEffects": false,
	"type": "module",
	"exports": {
		".": {
			"import": "./dist/index.mjs"
		}
	},
	"bin": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"files": [
		"dist"
	],
	"scripts": {
